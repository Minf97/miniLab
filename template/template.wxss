/* template/template.wxss */

.navigate_item{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  line-height: 100rpx;
}
.navigate_item image:first-child{
  width: 50rpx;
  height: 50rpx;
  padding: 0 0 0 20rpx;
  padding: 0 20rpx;
}
.navigate_item text{
  flex: 1;
  text-align: start;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.navigate_item image:last-child{
  width: 30rpx;
  height: 30rpx;
  padding: 0 20rpx;
}
/* userInfo_item */
.userInfo_item{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  line-height: 100rpx;
}
.userInfo_item image{
  width: 50rpx;
  height: 50rpx;
  padding: 0 20rpx 0 20rpx;
}
/* 不知道为什么 .userInfo_item text:first-child 选不到它，只好加个类名了 */
.userInfo_item .title{
  flex: 1;
  text-align: start;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.userInfo_item text:last-child{
  margin-right: 20rpx;
  font-size: 20rpx;
  color: rgb(186, 186, 186);
  letter-spacing: 2rpx;
}

.labList_item{
   display: flex;
   flex-direction: row;
   justify-content: space-between;
   align-items: center;
   line-height: 120rpx;
 }
 .labList_item image:first-child{
   width: 60rpx;
   height: 60rpx;
   padding: 0 10rpx 0 20rpx;
}
.labList_item text{
  flex: 1;
  text-align: start;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: rgb(106, 106, 106);
  
}
.labList_item image:last-child{
  width: 30rpx;
  height: 30rpx;
  padding: 0 20rpx;
}

/* labProject */
.labProject_item {
  width: 100%;
  height: 120rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.labProject_item image {
  width: 110rpx;
  height: 110rpx;
  padding: 0 10rpx 0 20rpx;
  background-color: green;
}